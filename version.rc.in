 PRODUCTVERSION @UNPC_VER_YEAR@,@UNPC_VER_MONTH@,@UNPC_VER_DAY@,@UNPC_VER_BUILD@
 FILEFLAGSMASK 0x3fL
 FILEFLAGS 0x0L
 FILEOS 0x40004L
 FILETYPE 0x2L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "ProductName", "UnhideNPCs"
            VALUE "FileDescription", "https://github.com/server-imp/UnhideNPCs"
            VALUE "FileVersion", "@UNPC_VER_STRING@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 1200
    END
END